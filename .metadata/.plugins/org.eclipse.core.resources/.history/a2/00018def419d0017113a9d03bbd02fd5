public class Main {
	public static void main(String[] args) {
		int fatorial = 1;
		long fatorialLong=1;
		
		for(int i=1;i<=40;i++) {
			fatorial= fatorial * i;
			fatorialLong= fatorialLong * i;
			System.out.println("fatorial de " +i+ ": " + fatorial);
			System.out.println("fatorialLong de " +i+ ": " + fatorialLong);
		}
	}
}
